class DrumKitTestDrive {
	public static void main(String[] args) {
		DrumKit d = new DrumKit();
		boolean topHat = true;
		boolean snare = true;
		d.snare = false;
		if(d.Snare == true){
			d.playSnare();
		}
		d.playSnare();
		d.playTopHat();
	
	}

}

class DrumKit{
	void playSnare(){
		System.out.println("bang bang ba-bang");
	}
	void playTopHat(){
		System.out.println("dung ding da-ding");
	}
	
}
