/* CALLING METHOD
 *double result;
 *result = square(6.0);
 */

public class BasicMath {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		 public static double square (double a){
			 double answer;
			 answer = a * a;
			 return answer;
		 }
	
		 public static double cube (double a){
			 double answer;
			 answer = a * a * a;
			 return answer;
		 }
	}
}
